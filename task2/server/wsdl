  <portType name="CarsPort">
    <operation name="getCars">
      <input message="tns:getCarsIn"/>
      <output message="tns:getCarsOut"/>
    </operation>
    <operation name="getCarById">
      <input message="tns:getCarByIdIn"/>
      <output message="tns:getCarByIdOut"/>
