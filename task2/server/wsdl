      </input>
      <output>
        <soap:body use="encoded" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" namespace="http://soap.test/task2/server/server.php"/>
      </output>
    </operation>
  </binding>
  <service name="CarsService">
    <port name="CarsPort" binding="tns:CarsBinding">
      <soap:address location="http://soap.test/task2/server/server.php"/>
    </port>
  </service>
  <message name="getCarsIn"/>
  <message name="getCarsOut">
    <part name="return" type="xsd:string"/>
  </message>
  <message name="getCarByIdIn">
    <part name="id" type="xsd:anyType"/>
  </message>
  <message name="getCarByIdOut">
    <part name="return" type="xsd:string"/>
