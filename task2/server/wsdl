      </output>
    </operation>
    <operation name="getCarById">
      <soap:operation soapAction="http://soap.test/task2/server/server.php#getCarById"/>
      <input>
        <soap:body use="encoded" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" namespace="http://soap.test/task2/server/server.php"/>
      </input>
      <output>
        <soap:body use="encoded" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" namespace="http://soap.test/task2/server/server.php"/>
      </output>
